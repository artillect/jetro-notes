Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox["T", 
    SuperscriptBox["T", "*"]], "=", 
   SuperscriptBox[
    RowBox[{
     SuperscriptBox["M", "2"], "(", 
     FractionBox[
      RowBox[{"1", "+", "\[Gamma]"}], 
      RowBox[{"1", "+", 
       RowBox[{"\[Gamma]", " ", 
        SuperscriptBox["M", "2"]}]}]], ")"}], "2"]}], 
  TraditionalForm]], "Text",
 CellChangeTimes->{{3.8576714120099916`*^9, 3.8576714483411074`*^9}, 
