     RowBox[{"(", 
      SubscriptBox["T", "0"], ")"}], "*"]], "=", 
   FractionBox[
    RowBox[{"2", " ", 
     RowBox[{
      SuperscriptBox["M", "2"], "(", 
      RowBox[{"1", "+", "\[Gamma]"}], ")"}], 
     RowBox[{"(", 
