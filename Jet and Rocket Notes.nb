    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"1", "+", 
        RowBox[{"\[Gamma]", " ", 
         SubsuperscriptBox["M", "1", "2"]}]}], 
       RowBox[{"1", "+", 
        RowBox[{"\[Gamma]", " ", 
         SubsuperscriptBox["M", "2", "2"]}]}]], ")"}], "2"], " ", 
    FractionBox[
     SubsuperscriptBox["M", "2", "2"], 
     SubsuperscriptBox["M", "1", "2"]]}]}], TraditionalForm]], "Text",
 CellChangeTimes->{
  3.855326691657469*^9, {3.8576711614494796`*^9, 3.857671219832105*^9}, {
