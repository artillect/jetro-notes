Cell["Compressible flow in constant diameter pipes", "Section",
 CellChangeTimes->{{3.857670999343957*^9, 
  3.8576710092105165`*^9}},ExpressionUUID->"78b91b0c-17d8-46fb-91ec-\
b4fca5284a4d"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    SubscriptBox["P", "1"], 
    SubscriptBox["P", "2"]], "=", 
   FractionBox[
    RowBox[{"1", "+", 
     RowBox[{"\[Gamma]", " ", 
      SubsuperscriptBox["M", "2", "2"]}]}], 
    RowBox[{"1", "+", 
     RowBox[{"\[Gamma]", " ", 
      SubsuperscriptBox["M", "1", "2"]}]}]]}], TraditionalForm]], "Text",
 CellChangeTimes->{
  3.855326691657469*^9, {3.8576710521146193`*^9, 3.8576710795637665`*^9}, {
   3.8576712246711445`*^9, 3.857671282850562*^9}, {3.857671856162448*^9, 
   3.857671856624447*^9}},ExpressionUUID->"afecf424-93c6-45f5-958f-\
08ab15f60e31"],

Cell["If somewhere along the pipe M=1", "Text",
 CellChangeTimes->{
  3.855326691657469*^9, {3.857671090114808*^9, 
   3.8576711024388504`*^9}},ExpressionUUID->"95531495-e463-4365-8426-\
3ec6af5a943e"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox["P", 
    SuperscriptBox["P", "*"]], "=", 
   FractionBox[
    RowBox[{"1", "+", "\[Gamma]"}], 
    RowBox[{"1", "+", 
     RowBox[{"\[Gamma]", " ", 
      SuperscriptBox["M", "2"]}]}]]}], TraditionalForm]], "Text",
 CellChangeTimes->{
  3.855326691657469*^9, {3.857671104200849*^9, 3.857671125675846*^9}, 
   3.8576712221051044`*^9, {3.857671284671561*^9, 3.8576712954916153`*^9}, 
   3.857671854871447*^9},ExpressionUUID->"464f3902-c495-4153-976e-\
ea7e39c4abbb"],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    SubscriptBox["P", "0"], 
    SuperscriptBox[
     SubscriptBox["P", "0"], "*"]], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"1", "+", "\[Gamma]"}], 
     RowBox[{"1", "+", 
      RowBox[{"\[Gamma]", " ", 
       SuperscriptBox["M", "2"]}]}]], 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{"1", "+", 
        RowBox[{
         FractionBox[
          RowBox[{"\[Gamma]", "-", "1"}], "2"], 
         SuperscriptBox["M", "2"]}]}], 
       FractionBox[
        RowBox[{"\[Gamma]", "+", "1"}], "2"]], ")"}], 
     FractionBox["\[Gamma]", 
      RowBox[{"\[Gamma]", "-", "1"}]]]}]}], TraditionalForm]], "Text",
 CellChangeTimes->{
  3.855326691657469*^9, {3.857671464894189*^9, 3.8576715493184614`*^9}, 
   3.857671854089447*^9},ExpressionUUID->"8e60a1e0-4e06-4ebe-aa7a-\
f6a1e9a32dd7"],

